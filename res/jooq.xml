<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<configuration xmlns="http://www.jooq.org/xsd/jooq-codegen-3.11.0.xsd">
	<!-- Configure the database connection here -->
	<generator>
		<generate>
			<pojos>true</pojos>
			<globalCatalogReferences>false</globalCatalogReferences>
			<globalKeyReferences>false</globalKeyReferences>
			<globalLinkReferences>false</globalLinkReferences>>
			<globalQueueReferences>false</globalQueueReferences>
			<globalRoutineReferences>false</globalRoutineReferences>
			<globalSchemaReferences>false</globalSchemaReferences>
			<globalSequenceReferences>false</globalSequenceReferences>
			<globalTableReferences>false</globalTableReferences>
			<globalUDTReferences>false</globalUDTReferences>
		</generate>

		<database>
			<excludes>
			</excludes>
			<includes>.*</includes>
			<inputSchema>germinate_template_3_7_0</inputSchema>
			<name>org.jooq.meta.mysql.MySQLDatabase</name>
		</database>

		<name>jhi.germinate.util.CustomCodegen</name>

		<target>
			<directory>src/</directory>
			<packageName>jhi.germinate.server.database</packageName>
		</target>
	</generator>

	<jdbc>
		<driver>com.mysql.cj.jdbc.Driver</driver>
		<password></password>
		<properties>
			<property>
				<key>user</key>
				<value>root</value>
			</property>
			<property>
				<key>password</key>
				<value></value>
			</property>
		</properties>
		<url>jdbc:mysql://localhost/germinate_template_3_7_0</url>

		<!-- You can also pass user/password and other JDBC properties in the optional properties tag: -->
		<user>root</user>
	</jdbc>
</configuration>