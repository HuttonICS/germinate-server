	<jdbc>
		<driver>com.mysql.cj.jdbc.Driver</driver>
		<password></password>
		<properties>
			<property>
				<key>user</key>
				<value>root</value>
			</property>
			<property>
				<key>password</key>
				<value></value>
			</property>
		</properties>
		<url>jdbc:mysql://localhost/germinate_template_3_6_0</url>

		<!-- You can also pass user/password and other JDBC properties in the optional properties tag: -->
		<user>root</user>
	</jdbc>
</configuration>